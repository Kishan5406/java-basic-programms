package com.company;

import java.sql.SQLOutput;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	// write your code here
        Scanner in =new Scanner(System.in);
        int num1=in.nextInt();
        int num2 = in.nextInt();
        int n1=num1;
      int  n2=num2;
        while(num1!=0){
            int rem=num2%num1;
            num2=num1;
            num1=rem;

        }
        System.out.println("HCF is "+num2);
        int LCM=(n1*n2)/num2;
        System.out.println("LCM is "+LCM);
    }
}
